: let cor; let circuloX; // horizontal  let circuloY; // vertical  function setup() {   random()   createCanvas(400, 400);   background(color(100,0,0));   cor = color(random(0,225), random(0,225), random(0, 225));   circuloX = [0,0,0];   circuloY = [random(height), random(height), random(height)];    }  function draw(){    fill(cor);      //console.lrandom0,225), random(0,225), random(0, 225), random(0,100));   }  }
